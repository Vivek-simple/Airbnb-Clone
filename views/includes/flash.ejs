<%if(success && success.length){%>
<div class="alert alert-success col-6 offset-3 mt-1 alert-dismissible fade show new-listing " role="alert">
    <button type="button" class="btn-close" data-bs-dismiss="alert" ></button>
   <%=success%>
  </div>
<%}%>

<%if(error && error.length){%>
    <div class="alert alert-danger col-6 offset-3 mt-1 alert-dismissible fade show new-listing " role="alert">
        <button type="button" class="btn-close" data-bs-dismiss="alert" ></button>
       <%=error%>
      </div>
    <%}%>